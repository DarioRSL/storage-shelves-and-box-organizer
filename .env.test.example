# Test Environment Configuration
# Copy this file to .env.test and fill in your local Supabase credentials
#
# NOTE: Currently uses same Supabase instance as development
# Future: Will use separate test instance on different ports

# Test Supabase API URL (using dev instance for now)
TEST_SUPABASE_URL=http://127.0.0.1:54321
PUBLIC_SUPABASE_URL=http://127.0.0.1:54321

# Test Supabase Keys
# Get these from: supabase status (when running locally)
TEST_SUPABASE_ANON_KEY=your_local_supabase_anon_key_here
PUBLIC_SUPABASE_ANON_KEY=your_local_supabase_anon_key_here

# Test Supabase Service Role Key (full admin access, for test setup/teardown)
TEST_SUPABASE_SERVICE_ROLE_KEY=your_local_supabase_service_role_key_here
SUPABASE_SERVICE_ROLE_KEY=your_local_supabase_service_role_key_here

# Test Database Connection (using dev database)
TEST_DB_HOST=127.0.0.1
TEST_DB_PORT=54322
TEST_DB_USER=postgres
TEST_DB_PASSWORD=postgres
TEST_DB_NAME=postgres

# Test API Configuration (using dev API)
TEST_API_PORT=54321
TEST_API_URL=http://127.0.0.1:54321

# Node Environment
NODE_ENV=test

# Application URL for tests
APP_URL=http://localhost:4321
